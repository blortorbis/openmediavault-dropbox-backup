[Unit]
Description=OpenMediaVault Dropbox Backup Timer
Documentation=https://github.com/yourusername/openmediavault-dropbox-backup

[Timer]
# Timer schedule is managed by the OMV module
# Default: daily at 2:00 AM
OnCalendar=*-*-* 02:00:00
RandomizedDelaySec=1800
Persistent=true

[Install]
WantedBy=timers.target
